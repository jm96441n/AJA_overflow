  <form id="ask-button" method="GET" action="/questions/new">
<div id="ask-button-homepage" class="button small-6 large-2 columns homepage-questions">
    <input id="ask_button" type="submit" value="ASK">
</div>
  </form>

  <form id="find-questions-button" method="GET" action="/questions/index">
<div id="find-button-homepage" class="button small-6 large-2 columns homepage-questions">
    <input id="find_questions_button" type="submit" value="FIND ANSWERS">
</div>
  </form>

<div id="homepage-header-photo">
  <img src='http://cdn.pcwallart.com/images/yoda-star-wars-wallpaper-wallpaper-4.jpg'>
</div>

<div id="login-form-container">
  <% if @errors %>
    <%= @errors[0] %>
  <% end %>

  <form id='login_form' action='/login' method='post'>
      <div class='login-div'>
        <input type='text' name='user[username]' placeholder='username' >
      </div>
      <div class='login-div'>
        <input type='password' name='user[password]' placeholder='password'>
      </div>
      <div id='submit-button' class='login-div'>
        <input type='submit' value='Log In'>
      </div>
  </form>
</div>